{
  "name": "vite-vue-plugin-theme",
  "version": "1.0.0",
  "packageManager": "pnpm@7.11.0",
  "description": "Vite plugin for dynamically changing the theme color of the interface",
  "author": "xingyuv <xingyu4j@vip.qq.com> (https://github.com/xingyuv)",
  "license": "MIT",
  "keywords": [
    "vite"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "es",
    "CHANGELOG.md",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es",
    "dev": " tsup src/index.ts --dts --format cjs,esm --watch",
    "dev:client": "tsc -w --p tsconfig.client.json",
    "build": "npm run clean  && tsup src/index.ts --dts --format cjs,esm && tsc -p tsconfig.client.json",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "example:dev": "npm -C example run serve",
    "example:build": "npm -C example run build",
    "release:prepare": "shipjs prepare",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vite": ">=4.2.0"
  },
  "dependencies": {
    "@types/node": "^18.15.6",
    "@types/tinycolor2": "^1.4.3",
    "clean-css": "^5.3.2",
    "debug": "^4.3.4",
    "esbuild": "^0.17.12",
    "esbuild-plugin-alias": "^0.2.1",
    "picocolors": "^1.0.0",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@kirklin/eslint-config": "^0.3.2",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.0",
    "@types/less": "^3.0.3",
    "@types/node": "^18.15.6",
    "@vue/compiler-sfc": "^3.2.47",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.36.0",
    "fs-extra": "^11.1.1",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "pnpm": "^7.30.0",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  }
}
